{
	"name": "base64image",
	"displayName": "base64image",
	"description": "将图片Base64",
	"version": "0.0.1",
	"publisher": "danyow",
	"author": {
		"name": "danyow",
		"email": "i.zuucol@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/danyow/base64image"
	},
	"homepage": "https://github.com/danyow/base64image/blob/master/README.md",
	"bugs": {
		"url": "https://github.com/danyow/base64image/issues"
	},
	"icon": "res/icon.png",
	"galleryBanner": {
		"color": "#252526",
		"theme": "dark"
	},
	"keywords": [
		"base64",
		"image",
		"markdown",
		"paste"
	],
	"engines": {
		"vscode": "^1.0.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.base64image"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.base64image",
				"title": "Base64 Image"
			}
		],
		"keybindings": [
			{
				"command": "extension.base64image",
				"key": "ctrl+alt+s",
				"mac": "cmd+ctrl+s",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"dependencies": {
		"moment": "^2.29.2"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.37.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	}
}
